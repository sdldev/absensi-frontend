---
import Student from "@/components/StudentImage.jsx";
import MainLayout from "@/layouts/Main.astro";
import Container from "@/components/Container.astro";

export interface Props {
  title?: string;
  description?: string;
  image?: string;
}
---

<MainLayout title="Upload Foto">
  <Container>
    <div class="p-6 mx-auto">
      <div class="mt-4">
        <img
          src="images/error.webp"
          alt="Preview"
          class="mx-auto rounded-lg"
          width="200"
        />
      </div>
      <div class="py-6 mx-auto">
        Masa Upload Foto berakhir, siswa yang belum upload foto harap
        hubungi Bp. Mahardika di ruang lab komputer
      </div>
    </div>
    <!-- <Student client:load /> -->
  </Container>
</MainLayout>
